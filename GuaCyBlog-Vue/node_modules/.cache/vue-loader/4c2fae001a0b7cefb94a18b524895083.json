e\">\n      <el-card class=\"article-card\">\n        <div slot=\"header\" align=\"left\">\n          <el-button\n              type=\"text\"\n              size=\"small\" @click=\"show(article)\">\n            {{article.articleTitle}}\n          </el-button>\n          <el-button\n              type=\"text\"\n              size=\"small\" @click=\"deleteText(article)\">\n            <i class=\"el-icon-delete\" float=\"right\"></i></el-button>\n        </div>\n        <div style=\"font-size:10px;\" align=\"left\">\n          {{article.articleInfo}}\n        </div>\n      </el-card>\n    </div>\n  </div>\n</template>\n<script>\nimport tag from '../components/admin/tag.vue'\nexport default {\n  inject: ['reload'],//注入依赖\n  data() {\n    return{\n      articleList:[],\n      commentList:[],\n      total: 0,\n      userId:window.sessionStorage.getItem(\"visitingId\"),\n      user:{\n        id:window.sessionStorage.getItem(\"visitingId\")\n      }\n    }\n  },\n  created() {\n      this.getArticleList();\n  },\n  methods:{\n    async getArticleList() {\n      const {data: res} = await this.$http.post(\"article/findArticleById\",this.user);\n      this.articleList = res.data;\n    },\n    async show(x) {\n       const {data:res}=await this.$http.post(\"article/allComment\",x);\n       this.commentList=res.comments;\n       this.$message_success(this.commentList);\n       await this.$router.push({path: '/show', query: {article: x, commentList: this.commentList}})\n    },\n    async deleteText(x) {\n      this.$confirm('确认删除文章?', {\n        confirmButtonClass: '确定',\n        cancelButtonText: '取消',\n      }).then(()=>{\n        const {data:res}=this.$http.post(\"article/delete\",x);\n        //this.reload();\n        this.$router.go(0);\n      }).catch(()=>{\n      });\n    },\n    goto(x) {\n      this.$router.push({path:x});\n    }\n  }\n}\n</script>\n<style>\n.el-container {\n  line-height: 50px;\n}\n.article-card{\n  width: 100%;\n}\n\n</style>"]}]}