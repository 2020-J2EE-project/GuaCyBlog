<el-col :span=\"2\"><div class=\"grid-content bg-purple\" @click=\"asideChange()\">|||</div></el-col>\n      </el-row>\n    </div>\n    <!--侧边栏-->\n    <el-container>\n      <div>\n        <el-aside :width=\"isCollapse?'0px':'201px'\" class=\"my-aside\">\n          <el-menu default-active=\"1-1\" class=\"el-menu-vertical-demo\"\n                   :collapse=\"isCollapse\">\n            <el-submenu index=\"1\" style=\"margin-left: -50px;\">\n              <template slot=\"title\">\n                <i class=\"el-icon-user\" />\n<!--                <span slot=\"title\" style=\"margin-left: -3px\">-->\n<!--                  <el-button-->\n<!--                    type=\"text\"-->\n<!--                    @click=\"goto('/usermain')\">-->\n<!--                    个人中心</el-button></span>-->\n                <span slog=\"title\" style=\"margin-left: -3px\">个人中心</span>\n              </template>\n              <el-menu-item-group>\n                <template slot=\"title\">my center</template>\n                <el-menu-item index=\"1-1\">\n                  <el-button\n                      type=\"text\"\n                      @click=\"goto('/user')\">信息修改</el-button>\n                </el-menu-item>\n                <el-menu-item index=\"1-2\">\n                  <el-button\n                      type=\"text\"\n                      @click=\"goto('/friend')\">我的好友</el-button></el-menu-item>\n                <el-menu-item index=\"1-3\">\n                  <el-button\n                    type=\"text\"\n                    @click=\"goto('/mail')\">消息中心</el-button></el-menu-item>\n              </el-menu-item-group>\n            </el-submenu>\n            <el-submenu index=\"1\" style=\"margin-left: -50px\">\n              <template slot=\"title\">\n                <i class=\"el-icon-document\" />\n                <span slog=\"title\" style=\"margin-left: -3px\">文章中心</span>\n              </template>\n              <el-menu-item-group>\n                <template slot=\"title\">my article</template>\n                <el-menu-item index=\"2-1\"><span slot=\"title\"><el-button\n                    type=\"text\"\n                    @click=\"goto('/article')\">文章列表</el-button></span></el-menu-item>\n                <el-menu-item index=\"2-2\"><span slot=\"title\"><el-button\n                    type=\"text\"\n                    @click=\"goto('/editor')\">新建文章</el-button></span></el-menu-item>\n              </el-menu-item-group>\n            </el-submenu>\n          </el-menu>\n        </el-aside>\n      </div>\n      <el-main class=\"my-main\">\n        <!--资料卡-->\n        <el-container direction=\"horizontal\">\n          <el-col span=\"40\" class=\"my-col\">\n            <el-card class=\"my-card\">\n              <el-avatar>\n                <img src=\"../assets/p1.jpg\">\n              </el-avatar>\n              <div>{{this.user.username}}</div>\n              <div class=\"intro-text\">{{this.user.intro}}</div>\n              <el-divider></el-divider>\n              <div>\n                <el-button type=\"text\" @click=\"goto('/article')\">\n                  文章数 {{this.user.articleNum}}\n                </el-button>\n                <el-divider direction=\"vertical\"></el-divider>\n                <el-button type=\"text\" @click=\"goto('/friend')\">\n                  粉丝数 {{this.user.followerNum}}\n                </el-button>\n                <el-divider direction=\"vertical\"></el-divider>\n                <el-button type=\"text\" @click=\"goto('/friend')\">\n                  关注数 {{this.user.followingNum}}\n                </el-button>\n              </div>\n              <el-divider>\n              </el-divider>\n              <!--标签展示-->\n              <el-col span=\"40\" style=\"line-height:40px\">\n                <div style=\"margin-top: -30px;\">\n                  <div class=\"tags-wrapper card-box\">\n                    <div class=\"tags\">\n                      <template v-for=\"(item, index) in tagList\" >\n                        <el-button  :class=\"isActive==index?'tagbtn':'tagbtn2'\"\n                                    type=\"text\" size=\"small\" @click=\"click(item.tagName,index)\"\n                        >\n                          {{item.tagName}}\n                        </el-button>\n                      </template>\n                    </div>\n                  </div>\n                </div>\n              </el-col>\n            </el-card>\n          </el-col>\n          <!--文章显示-->\n          <el-col span=\"100\" class=\"my-col2\">\n            <el-card class=\"my-card\">\n              <div v-if=\"articleList.length==0\">\n                <el-card class=\"article-card\">\n                  <div align=\"middle\">\n                    <h2>暂无文章</h2>\n                  </div>\n                </el-card>\n              </div>\n              <div v-else>\n                <h3>{{temp}}</h3>\n              </div>\n              <div v-for=\"article in articleList\" :key=\"article.articleTitle\">\n                <el-card class=\"article-card\">\n                  <div slot=\"header\" align=\"middle\">\n                    <el-button\n                        type=\"text\"\n                        size=\"small\"\n                        @click=\"show(article)\">\n                      {{article.articleTitle}}\n                    </el-button>\n                  </div>\n                  <div style=\"font-size:10px;\" align=\"middle\">\n                    {{article.articleInfo}}\n                  </div>\n                </el-card>\n              </div>\n            </el-card>\n          </el-col>\n        </el-container>\n      </el-main>\n    </el-container>\n  </el-container>\n</template>\n<script>\nimport tag from '../components/admin/tag.vue';\nexport default {\n  watch: {\n    $route(to, from) {\n      this.temp = this.$route.query.myTagName;\n    },\n  },\n  data() {\n    return {\n      articleList:[],\n      isCollapse: false,\n      allList:[],\n      user:{\n        id:window.sessionStorage.getItem(\"visitingId\")\n      },\n      connect:{\n        userId:window.sessionStorage.getItem(\"visitingId\"),\n        tagName:''\n      },\n      commentList:[],\n    }\n  },\n  components :{\n    tag\n  },\n  created() {\n    this.getUser();\n    this.getTags();\n    this.getArticleList();\n  },\n  mounted() {\n    this.temp = this.$refs.guaguatag.myTagName;\n  },\n  methods: {\n    asideChange(){\n      this.isCollapse = !this.isCollapse;\n    },\n    async getArticleList() {\n      // 调用post请求\n      const {data: res} = await this.$http.post(\"article/findArticleById\",this.user);\n      this.articleList = res.data; // 将返回数据赋值\n      this.allList = res.data;\n    },\n    async show(x) {\n      const {data:res}=await this.$http.post(\"article/allComment\",x);\n      this.commentList=res.comments;\n      this.$router.push({path:'/show',query:{article:x ,commentList:this.commentList}})\n    },\n    async getUser() {\n      //get-Info\n      const{data:res} = await this.$http.post(\"user/findById\",this.user);\n      this.user = res.data;\n    },\n    goto(x) {\n      this.$router.push({path:x});\n    },\n    async getTags(){\n      const { data: res } = await this.$http.post(\"tag/allTag\", this.user);\n      this.tagList=res.tags;\n      for (let i = 0, tagH = this.tagList.length; i < tagH; i++) {\n        this.tagStyleList.push(this.getTagStyle())\n      }\n    },\n    getTagStyle() {\n      const tagBgColor = this.tagBgColor\n      const randomColor = tagBgColor[Math.floor(Math.random() * tagBgColor.length)]\n      return `background: ${randomColor};--randomColor:${randomColor};`\n    },\n    async click(x,y){\n      this.connect.tagName=x;\n      if(this.isActive==y) {\n        this.isActive=-1\n        this.myTagName=\"All Blogs\"\n        this.articleList = this.allList;\n      }\n      else{\n        this.isActive=y;\n        this.myTagName=x;\n        this.connect.tagName = x;\n        const {data: res} = await this.$http.post(\"article/findByIdAndTag\",this.connect);\n        this.articleList = res.data;\n        // window.sessionStorage.setItem(\"myTag\",x);\n      }\n      //this.$router.push({path:'/articlebytag',query:{articleList:this.articleList,tagName:x}})\n    }\n  }\n}\n</script>\n\n\n\n<style lang='stylus'>\n.tags-wrapper\n  .title\n    color var(--textColor)\n    opacity 0.9\n    font-size 1.2rem\n    margin:auto\n  .tags\n    text-align justify\n    padding .8rem .5rem .5rem .5rem\n    margin:auto\n    a\n      opacity .8\n      display inline-block\n      padding .2rem .4rem\n      transition all .4s\n      background-color var(--textColor)\n      color var(--mainBg)\n      border-radius 3px\n      margin 0 .3rem .5rem 0\n      min-width 2rem\n      height 1rem\n      line-height 1rem\n      font-size .8rem\n      margin:auto\n      text-align center\n      @media (max-width: $MQMobile)\n        font-weight 400\n      &:hover\n        opacity 1\n        margin:auto\n        transform scale(1.1)\n      &.active\n        box-shadow: 0 5px 10px -5px var(--randomColor, rgba(0,0,0,0.15));\n        transform scale(1.22)\n        opacity 1\n        &:hover\n          text-decoration none\n    .tagbtn\n      height: 32px;\n      line-height: 30px;\n      padding-top: 0;\n      padding-bottom: 0;\n      margin:6px 6px 6px 6px;\n      size: auto;\n      font-weight : bold;\n      color: #ffffff\n      background: #a9ddf9;\n    --randomColor:#a9ddf9;\n    .tagbtn2\n      height: 32px;\n      line-height: 30px;\n      padding-top: 0;\n      padding-bottom: 0;\n      margin:6px 6px 6px 6px;\n      size: auto;\n      font-weight : bold;\n      color: #ffffff;\n      background: #c4dae6;\n      --randomColor:#c4dae6;\n</style>\n<style>\n.el-menu-vertical-demo:not(.el-menu--collapse) {\n  width: 200px;\n  min-height: 400px;\n}\n.el-button{\n  width: 64px;\n}\n.el-card{\n  line-height: 30px;\n}\n.el-main{\n  font-family:Consolas;\n}\n.intro-text{\n  font-size: 10px;\n}\n\n.my-aside{\n  margin-top: 20px;\n  line-height: 50px;\n  margin-left: -5px;\n  margin-right: -5px;\n}\n.my-main{\n}\n.my-card{\n  alignment: center;\n  min-width: 300px;\n}\n.article-card{\n  alignment: center;\n  min-width: 500px;\n}\n.my-col{\n  float: unset;\n  margin-left: 50px;\n  margin-right: 50px;\n}\n.my-col2{\n  margin-left:0px;\n}\n</style>\n"]}]}