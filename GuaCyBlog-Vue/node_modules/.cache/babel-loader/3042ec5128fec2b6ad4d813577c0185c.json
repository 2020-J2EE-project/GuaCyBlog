  region: '',\r\n        date1: '',\r\n        date2: '',\r\n        delivery: false,\r\n        type: [],\r\n        resource: '',\r\n        desc: ''\r\n      },\r\n      fileList: []\r\n    }\r\n  },\r\n  created() {\r\n    this.getUser()\r\n  },\r\n  methods: {\r\n    onSubmit() {\r\n      console.log(this.User2.username);\r\n      const{data:res2} = this.$http.post(\"user/updateById\",this.User2);\r\n      this.$message_success(\"用户信息修改成功!\");\r\n    },\r\n    async getUser() {\r\n        const{data:res} = await this.$http.post(\"user/findById\",this.user);\r\n        this.User2 = res.data;\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n"],"sourceRoot":"src/components/admin"}]}