A,KAAA,CAAA,aAAA,CAAA,QAAA;AAAA,2EAAA,iBAAA,KAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,KADA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAEA,kBAAA,KAAA,CAAA,UAAA,CAAA,IAAA,GAAA,KAAA,CAAA,KAAA,CAAA,QAAA,CAAA,WAAA,CAFA,CAGA;;AAHA;AAAA,yBAIA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA,KAAA,CAAA,UAAA,CAJA;;AAAA;AAAA;AAIA,kBAAA,GAJA,yBAIA,IAJA;;AAKA,kBAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA;;AALA;AAAA,yBAMA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,oBAAA,IAAA,EAAA;AAAA,mBAAA,CANA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAQA;AAVA;AA1BA,CAAA","sourcesContent":["<template>\r\n  <div class=\"editir_container\">\r\n    <div class=\"editor_box\">\r\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n        <el-breadcrumb-item :to=\"{ path: '/welcome' }\">首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item :to=\"{ path: '/editor' }\">新建文章</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n      <!--添加表单-->\r\n      <el-form\r\n          ref=\"editorFormRef\"\r\n          :model=\"editorForm\"\r\n          :rules=\"editorRules\"\r\n          label-width=\"0px\"\r\n      >\r\n        <el-form-item prop=\"articleTitle\">\r\n          <el-input v-model=\"editorForm.articleTitle\" ></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item prop=\"tag\">\r\n          <template>\r\n            <TagInput ref=\"tagInput\" v-model='editorForm.tags'/>\r\n          </template>\r\n        </el-form-item>\r\n        摘要\r\n        <el-form-item prop=\"articleInfo\">\r\n          <el-input type=\"textarea\" v-model=\"editorForm.articleInfo\" ></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"articleText\">\r\n          <mavon-editor v-model=\"editorForm.articleText\"/>\r\n        </el-form-item>\r\n\r\n        <el-form-item class=\"btns\">\r\n          <el-button type=\"primary\" @click=\"addArticle\">发布</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TagInput from '../components/TagInput.vue'\r\nexport default {\r\n  data() {\r\n    return {\r\n      editorForm:{\r\n        userId : window.sessionStorage.getItem(\"userId\"),\r\n        articleTitle:'文章标题',\r\n        tag:'',\r\n        articleInfo:'文章简介',\r\n        articleText: '',\r\n        articleTime:''\r\n        //defaultData: \"preview\"\r\n      },\r\n      editorRules: {\r\n        articleTitle: [\r\n          { required: true, message: \"文章标题不可为空\", trigger: \"blur\" },\r\n          {  max: 100, message: \"长度在不能大于100个字符\", trigger: \"blur\" }\r\n        ],\r\n        articleText : [\r\n          { required: true, message: \"文章内容不能为空\", trigger: \"blur\" },\r\n        ]\r\n      }\r\n    };\r\n  },\r\n  components :{\r\n    TagInput\r\n  },\r\n  methods:{\r\n    addArticle(){\r\n      this.$refs.editorFormRef.validate(async valid => {\r\n        if (!valid) return ;\r\n        this.editorForm.tags=this.$refs.tagInput.dynamicTags;\r\n        // 调用get请求\r\n        const {data :res} = await this.$http.post(\"article/addText\", this.editorForm);\r\n        this.$message.success(res);\r\n        await this.$router.push({path: '/article'});\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}