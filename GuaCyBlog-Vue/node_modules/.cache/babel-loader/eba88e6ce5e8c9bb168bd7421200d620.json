rrent-change=\"handleCurrentChange\"\r\n          :current-page=\"queryInfo.pageNum\"\r\n          :page-sizes=\"[1, 2, 5, 100]\"\r\n          :page-size=\"queryInfo.pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\"\r\n      ></el-pagination>\r\n    </el-card>\r\n\r\n    <!-- �������û��Ի��� -->\r\n    <el-dialog title=\"���Ӻ���\" :visible.sync=\"addDialogVisible\" width=\"50%\"\r\n               @close=\"addDialogClosed\">\r\n      <!-- ������������ -->\r\n      <el-form :model=\"addForm\" :rules=\"addFormRules\"\r\n               ref=\"addFormRef\" label-width=\"70px\">\r\n        <!-- �û��� -->\r\n        <el-form-item label=\"�û���\" prop=\"username\">\r\n          <el-input v-model=\"addForm.username\"></el-input>\r\n        </el-form-item>\r\n        <!-- ���� -->\r\n        <el-form-item label=\"����\" prop=\"password\">\r\n          <el-input v-model=\"addForm.password\"></el-input>\r\n        </el-form-item>\r\n        <!-- ���� -->\r\n        <el-form-item label=\"����\" prop=\"email\">\r\n          <el-input v-model=\"addForm.email\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <!-- ���ݵײ����� -->\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"addDialogVisible = false\">ȡ ��</el-button>\r\n        <el-button type=\"primary\" @click=\"addUser\">ȷ ��</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <!-- �޸��û��Ի��� -->\r\n    <el-dialog title=\"�޸��û���Ϣ\" :visible.sync=\"editDialogVisible\" width=\"50%\" @colse=\"editDialogClosed\">\r\n      <el-form :model=\"editForm\" :rules=\"editFormRules\" ref=\"editFormRef\" label-width=\"70px\">\r\n        <!-- �û��� -->\r\n        <el-form-item label=\"�û���\" prop=\"username\">\r\n          <el-input v-model=\"editForm.username\" disabled></el-input>\r\n        </el-form-item>\r\n        <!-- ���� -->\r\n        <el-form-item label=\"����\" prop=\"password\">\r\n          <el-input v-model=\"editForm.password\"></el-input>\r\n        </el-form-item>\r\n        <!-- ���� -->\r\n        <el-form-item label=\"����\" prop=\"email\">\r\n          <el-input v-model=\"editForm.email\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editDialogVisible = false\">ȡ ��</el-button>\r\n        <el-button type=\"primary\" @click=\"editUserInfo\">ȷ ��</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      // ��������\r\n      queryInfo: {\r\n        query: \"\",\r\n        pageNum: 1,\r\n        pageSize: 5\r\n      },\r\n      userlist: [],// �û��б�\r\n\r\n      total: 0, // ������ݼ�¼\r\n      addDialogVisible: false ,// �Ի�����ʾ\r\n      // �����û�������\r\n      addForm: {\r\n        username:'',\r\n        password:'',\r\n        email:'',\r\n      },\r\n      // �����޸��û��Ի�����ʾ/����\r\n      editDialogVisible:false,\r\n      // �޸��û���Ϣ\r\n      editForm:{},\r\n      // ��֤����\r\n      addFormRules:{\r\n        username:[\r\n          { required: true, message: \"�������û���\", trigger: \"blur\" },\r\n          { min: 5, max: 8, message: \"������ 5 �� 8 ���ַ�\", trigger: \"blur\" }\r\n        ],\r\n        password:[\r\n          { required: true, message: \"����������\", trigger: \"blur\" },\r\n          { min: 6, max: 8, message: \"������ 6 �� 8 ���ַ�\", trigger: \"blur\" }\r\n        ],\r\n        email:[\r\n          { required: true, message: \"����������\", trigger: \"blur\" },\r\n          { min: 5, max: 15, message: \"��������ȷ�����ַ\", trigger: \"blur\" }\r\n        ],\r\n      },\r\n      // �޸��û�������֤����\r\n      editFormRules:{\r\n        password:[\r\n          { required: true, message: \"����������\", trigger: \"blur\" },\r\n          { min: 6, max: 8, message: \"������ 6 �� 8 ���ַ�\", trigger: \"blur\" }\r\n        ],\r\n        email:[\r\n          { required: true, message: \"����������\", trigger: \"blur\" },\r\n          { min: 5, max: 15, message: \"��������ȷ�����ַ\", trigger: \"blur\" }\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    this.getUserList();\r\n  },\r\n  methods: {\r\n    async getUserList() {\r\n      // ����post����\r\n      const { data: res } = await this.$http.get(\"allUser\", {\r\n        params: this.queryInfo\r\n      });\r\n      this.userlist = res.data; // ���������ݸ�ֵ\r\n      this.total = res.numbers; // �ܸ���\r\n    },\r\n    // ����pageSize�ı���¼�\r\n    handleSizeChange(newSize) {\r\n      this.queryInfo.pageSize = newSize;\r\n      this.getUserList(); // ���ݷ����ı�������������\r\n    },\r\n    // ����pageNum�ı���¼�\r\n    handleCurrentChange(newPage) {\r\n      this.queryInfo.pageNum = newPage;\r\n      this.getUserList(); // ���ݷ����ı�������������\r\n    },\r\n    // �޸��û�״̬\r\n    async userStateChanged(userinfo) {\r\n      const { data: res } = await this.$http.put(\r\n          `userState?id=${userinfo.id}&state=${userinfo.state}`\r\n      );\r\n      if (res != \"success\") {\r\n        userinfo.state = !userinfo.state;\r\n        return this.$message.error(\"����ʧ�ܣ�����\");\r\n      }\r\n      this.$message.success(\"�����ɹ�������\");\r\n    },\r\n    // ���������û�\r\n    addDialogClosed(){\r\n      this.$refs.addFormRef.resetFields();// ���ñ�����\r\n    },\r\n    // �����û�\r\n    addUser(){\r\n      this.$refs.addFormRef.validate(async valid =>{\r\n        console.log(valid);\r\n        if( !valid ) return;\r\n        // ��������\r\n        const {data:res} = await this.$http.post(\"addUser\",this.addForm);\r\n        if (res != \"success\") {\r\n          userinfo.state = !userinfo.state;\r\n          return this.$message.error(\"����ʧ�ܣ�����\");\r\n        }\r\n        this.$message.success(\"�����ɹ�������\");\r\n        //����\r\n        this.addDialogVisible = false;\r\n        this.getUserList();\r\n      })\r\n    },\r\n    // չʾ�޸Ŀ�\r\n    async showEditDialog(id){\r\n\r\n      const {data:res} = await this.$http.get(\"getUpdate?id=\"+id);\r\n      // if (res != \"success\") {\r\n      // userinfo.state = !userinfo.state;\r\n      // return this.$message.error(\"����ʧ�ܣ�����\");\r\n      // }\r\n      // this.$message.success(\"�����ɹ�������\");\r\n\r\n      this.editForm = res;\r\n      this.editDialogVisible = true;\r\n    },\r\n    // �رմ���\r\n    editDialogClosed(){\r\n      this.$refs.editFormRef.resetFields();\r\n    },\r\n    // ȷ���޸�\r\n    editUserInfo(){\r\n      this.$refs.editFormRef.validate(async valid =>{\r\n        console.log(valid);\r\n        if( !valid ) return;\r\n        // ��������\r\n        const {data:res} = await this.$http.put(\"editUser\",this.editForm);\r\n        console.log(res);\r\n        if (res != \"success\") return this.$message.error(\"����ʧ�ܣ�����\");\r\n        this.$message.success(\"�����ɹ�������\");\r\n        //����\r\n        this.editDialogVisible = false;\r\n        this.getUserList();\r\n      });\r\n    },\r\n    // ɾ����ť\r\n    async deleteUser(id){\r\n      // ����\r\n      const confirmResult = await this.$confirm('�˲���������ɾ�����û�, �Ƿ����?', '��ʾ', {\r\n        confirmButtonText: 'ȷ��',\r\n        cancelButtonText: 'ȡ��',\r\n        type: 'warning'\r\n      }).catch(err => err)\r\n      // �ɹ�ɾ��Ϊconfirm ȡ��Ϊ cancel\r\n      if(confirmResult!='confirm'){\r\n        return this.$message.info(\"��ȡ��ɾ��\");\r\n      }\r\n      const {data:res} = await this.$http.delete(\"deleteUser?id=\"+id);\r\n      if (res != \"success\") {\r\n        return this.$message.error(\"����ʧ�ܣ�����\");\r\n      }\r\n      this.$message.success(\"�����ɹ�������\");\r\n      this.getUserList();\r\n    },\r\n  }\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n</style>"],"sourceRoot":"src/components/admin"}]}