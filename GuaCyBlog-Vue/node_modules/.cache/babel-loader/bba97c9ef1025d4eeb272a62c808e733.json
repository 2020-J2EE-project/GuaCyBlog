,IAAA,QAAA,CAAA,IAAA,CAAA,SAAA;AACA,UAAA,YAAA,GAAA,QAAA,CAAA,cAAA,CAAA,cAAA,CAAA;;AACA,UAAA,GAAA,IAAA,GAAA,EAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,QAAA,GAAA,OAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,GAAA,GAAA,MAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,KAAA,GAAA,WAAA;AACA,OAJA,MAIA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,QAAA,GAAA,UAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,GAAA,GAAA,KAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,KAAA,GAAA,WAAA;AACA;AACA;AAjDA,GA1CA;AA6FA,EAAA,QAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,UAAA,IAAA,GAAA;AACA,QAAA,QAAA,EAAA,KADA;AACA;AACA,QAAA,WAAA,EAAA,SAFA;AAEA;AACA,QAAA,QAAA,EAAA,KAHA;AAIA,QAAA,YAAA,EAAA,KAJA;AAKA,QAAA,WAAA,EAAA;AALA,OAAA;AAOA,aAAA,IAAA;AACA;AAVA,GA7FA;AAyGA,EAAA,SAzGA,uBAyGA;AACA,IAAA,MAAA,CAAA,mBAAA,CAAA,QAAA,EAAA,KAAA,SAAA;AACA;AA3GA,CAAA","sourcesContent":["<template>\r\n  <el-container>\r\n    <el-aside>\r\n      <el-card shadow=\"never\" id=\"directoryDiv\">\r\n        <div slot=\"header\"><b>目录</b></div>\r\n        <div v-for=\"(item, index) in aData\">\r\n          <el-button\r\n              type=\"text\"\r\n              size=\"small\"\r\n              text-align=\"left\"\r\n              @click=\"url(item.id)\">\r\n<!--            <a href=\"javascript:void(0);\">{{item.name}}</a>-->\r\n            {{item.name}}\r\n          </el-button>\r\n        </div>\r\n      </el-card>\r\n    </el-aside>\r\n    <el-main>\r\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n        <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item :to=\"{ path: '/article' }\">文章列表</el-breadcrumb-item>\r\n        <el-breadcrumb-item :to=\"{ path: '/show' }\">{{article.articleTitle}}</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n<!--      <div align=\"center\">-->\r\n<!--        {{article.articleTitle}}-->\r\n<!--      </div>-->\r\n      <mavon-editor class=\"md\"\r\n                    :value=\"article.articleText\"\r\n                    :subfield = \"prop.subfield\"\r\n                    :defaultOpen = \"prop.defaultOpen\"\r\n                    :toolbarsFlag = \"prop.toolbarsFlag\"\r\n                    :editable=\"prop.editable\"\r\n                    :scrollStyle=\"prop.scrollStyle\"\r\n      >\r\n      </mavon-editor>\r\n      <el-container direction=\"vertical\"  v-theme:bg=\"'wide'\">\r\n        <!--评论区-->\r\n        <div v-for=\"comment in commentList\" :key=\"comment\">\r\n          <el-card class=\"box-card\">\r\n            <div slot=\"header\" class=\"clearfix\">\r\n              用户:{{comment.reviewerId}}\r\n            </div>\r\n            <p>{{comment.commentText}}</p>\r\n            <div class=\"bottom\">\r\n                <span>\r\n                <el-button type=\"text\" class=\"button\">回复</el-button>\r\n                <el-button type=\"text\" class=\"button\">删除</el-button>\r\n                </span>\r\n            </div>\r\n            <el-col class=\"time\">评论时间：{{comment.commentTime}}</el-col>\r\n          </el-card>\r\n        </div>\r\n        <!--发布评论-->\r\n        <el-form class=\"comment-container\" :model=\"commentForm\" ref=\"commentForm\"\r\n                 :rules=\"commentRules\" label-position=\"left\">\r\n          <h4 class=\"comment-title\">发布评论</h4>\r\n          <el-form-item prop=\"commentText\">\r\n            <el-input type=\"textarea\" :rows=\"3\" v-model=\"commentForm.commentText\" auto-complete=\"off\" placeholder=\"评论内容\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button class=\"comment-button\" type=\"primary\" @click=\"submitComment\">发布</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-container>\r\n    </el-main>\r\n  </el-container>\r\n</template>\r\n\r\n\r\n<script>\r\nimport 'github-markdown-css/github-markdown.css'\r\nexport default {\r\n  name : 'showArticle',\r\n  data() {\r\n    return {\r\n      aData:[],\r\n      article:this.$route.query.article,\r\n      commentList :this.$route.query.commentList,\r\n      user: {\r\n        id:window.sessionStorage.getItem(\"userId\"),\r\n        username:window.sessionStorage.getItem(\"username\"),\r\n      },\r\n      //评论格式\r\n      commentForm: {\r\n        reviewerId:'' ,\r\n        articleId:'',\r\n        commentText:'',\r\n        commentTime:''\r\n      },\r\n      //信息格式\r\n      infoForm: {\r\n        postUserId:window.sessionStorage.getItem(\"userId\"),\r\n        receiveUserId:'',\r\n        type:'',\r\n        articleId:'',\r\n        isRead:0\r\n      },\r\n      //评论规则\r\n      commentRules: {\r\n        commentText: [\r\n          { required: true, message: \"评论内容不得为空\", trigger: \"blur\" },\r\n          { max: 100, message: \"长度在 100 个字符以内\", trigger: \"blur\" }\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.listA();\r\n    window.addEventListener(\"scroll\", this.directory);\r\n  },\r\n  created() {\r\n    // this.listA()\r\n  },\r\n  methods: {\r\n    url(url) {\r\n      document.querySelector('#' + url).scrollIntoView(true);\r\n    },\r\n    listA() {\r\n      this.$nextTick(function () {\r\n        let k = 0;\r\n        for(let i=0; i<document.getElementsByTagName(\"a\").length; i++) {\r\n          if (document.getElementsByTagName(\"a\")[i].id !== '') {\r\n            let aData = {\r\n              'id' : document.getElementsByTagName(\"a\")[i].id,  // 获取a标签的id\r\n              'name' : document.getElementsByTagName(\"a\")[i].parentNode.innerText\r\n              //获取这个a标签父节点的文字（即我们的标题）\r\n            };\r\n            this.aData.splice(i, 1, aData);\r\n            k++;\r\n          }\r\n        }\r\n        this.$message_success(k);\r\n      })\r\n    },\r\n    async submitComment() {\r\n      //处理评论\r\n      this.commentForm.articleId = this.article.articleId;\r\n      this.commentForm.reviewerId = this.article.userId;\r\n      const {data: res1} = await this.$http.post(\"article/addComment\", this.commentForm);\r\n      //更新评论\r\n      const {data: res2} = await this.$http.post(\"article/allComment\", this.article);\r\n      this.commentList = res2.comments;\r\n      this.$message_success(this.commentList);\r\n      //处理通知\r\n      this.infoForm.receiveUserId = this.article.userId;\r\n      this.infoForm.type = '2';\r\n      this.infoForm.articleId = this.article.articleId;\r\n      const {data: res3} = await this.$http.post(\"info/addInfo\", this.infoForm);\r\n      this.$message_success(res3);\r\n    },\r\n    directory() {\r\n      var top=document.documentElement.scrollTop||document.body.scrollTop;\r\n      var directoryDiv=document.getElementById(\"directoryDiv\");\r\n      if(top>= 164){\r\n        directoryDiv.style.position = \"fixed\";\r\n        directoryDiv.style.top = \"50px\";\r\n        directoryDiv.style.width = \"229.313px\";\r\n      }else{\r\n        directoryDiv.style.position = \"absolute\";\r\n        directoryDiv.style.top = \"0px\";\r\n        directoryDiv.style.width = \"229.313px\";\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    prop () {\r\n      let data = {\r\n        subfield: false,// 单双栏模式\r\n        defaultOpen: 'preview',//edit： 默认展示编辑区域 ， preview： 默认展示预览区域\r\n        editable: false,\r\n        toolbarsFlag: false,\r\n        scrollStyle: true\r\n      }\r\n      return data\r\n    }\r\n  },\r\n  destroyed() {\r\n    window.removeEventListener(\"scroll\", this.directory);\r\n  }\r\n}\r\n</script>"],"sourceRoot":"src/components"}]}