A,IAAA,CAAA,IAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA,KAPA;AAQA,IAAA,cARA,4BAQA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,oBAAA,EAAA,MAAA,CAAA,OAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAEA,gBAAA,MAAA,CAAA,UAAA,GAAA,GAAA,CAAA,UAAA;AACA,gBAAA,MAAA,CAAA,WAAA,GAAA,GAAA,CAAA,QAAA;AACA,gBAAA,MAAA,CAAA,WAAA,CAAA,SAAA,GAAA,GAAA,CAAA,SAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,WAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA,KAdA;AAeA,IAAA,GAfA,eAeA,IAfA,EAeA;AACA,MAAA,QAAA,CAAA,aAAA,CAAA,MAAA,IAAA,EAAA,cAAA,CAAA,IAAA;AACA,KAjBA;AAkBA,IAAA,aAlBA,2BAkBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,uBAAA,EAAA,MAAA,CAAA,WAAA,CADA;;AAAA;AAAA;AACA,gBAAA,IADA,yBACA,IADA;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,oBAAA,EAAA,MAAA,CAAA,OAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,IAFA,0BAEA,IAFA;AAGA,gBAAA,MAAA,CAAA,WAAA,GAAA,IAAA,CAAA,QAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KAtBA;AAuBA,IAAA,KAvBA,mBAuBA;AAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,YAAA,CAAA,GAAA,QAAA,CAAA,oBAAA,CAAA,GAAA,EAAA,MAAA;AACA,YAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AACA,YAAA,CAAA,GAAA,CAAA;;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,QAAA,CAAA,oBAAA,CAAA,GAAA,EAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA;AACA,cAAA,QAAA,CAAA,oBAAA,CAAA,GAAA,EAAA,CAAA,EAAA,EAAA,KAAA,EAAA,EAAA;AACA,YAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AACA,gBAAA,IAAA,GAAA;AACA,oBAAA,QAAA,CAAA,oBAAA,CAAA,GAAA,EAAA,CAAA,EAAA,EADA;AACA;AACA,sBAAA,QAAA,CAAA,oBAAA,CAAA,GAAA,EAAA,CAAA,EAAA,UAAA,CAAA,SAFA,CAEA;;AAFA,aAAA;AAIA,iBAAA,KAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA,EAAA,IAAA;AACA,gBAAA,CAAA,IAAA,CAAA,GAAA,CAAA,EACA;AACA;AACA;AACA,OAjBA;AAkBA,KA1CA;AA2CA,IAAA,aA3CA,2BA2CA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA;AACA,gBAAA,MAAA,CAAA,WAAA,CAAA,SAAA,GAAA,MAAA,CAAA,OAAA,CAAA,SAAA;AACA,gBAAA,MAAA,CAAA,WAAA,CAAA,UAAA,GAAA,MAAA,CAAA,OAAA,CAAA,MAAA;AAHA;AAAA,uBAIA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,oBAAA,EAAA,MAAA,CAAA,WAAA,CAJA;;AAAA;AAAA;AAIA,gBAAA,IAJA,yBAIA,IAJA;AAAA;AAAA,uBAMA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,oBAAA,EAAA,MAAA,CAAA,OAAA,CANA;;AAAA;AAAA;AAMA,gBAAA,IANA,0BAMA,IANA;AAOA,gBAAA,MAAA,CAAA,WAAA,GAAA,IAAA,CAAA,QAAA;AACA,gBAAA,MAAA,CAAA,UAAA,GAAA,IAAA,CAAA,UAAA;AACA,gBAAA,MAAA,CAAA,WAAA,CAAA,SAAA,GAAA,IAAA,CAAA,SAAA,CATA,CAUA;;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,aAAA,GAAA,MAAA,CAAA,OAAA,CAAA,MAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,IAAA,GAAA,GAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,SAAA,GAAA,MAAA,CAAA,OAAA,CAAA,SAAA;AAbA;AAAA,uBAcA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,cAAA,EAAA,MAAA,CAAA,QAAA,CAdA;;AAAA;AAAA;AAcA,gBAAA,IAdA,0BAcA,IAdA;;AAeA,gBAAA,MAAA,CAAA,gBAAA,CAAA,IAAA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA,KA3DA;AA4DA,IAAA,SA5DA,uBA4DA;AACA,UAAA,GAAA,GAAA,QAAA,CAAA,eAAA,CAAA,SAAA,IAAA,QAAA,CAAA,IAAA,CAAA,SAAA;AACA,UAAA,YAAA,GAAA,QAAA,CAAA,cAAA,CAAA,cAAA,CAAA;;AACA,UAAA,GAAA,IAAA,GAAA,EAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,QAAA,GAAA,OAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,GAAA,GAAA,MAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,KAAA,GAAA,WAAA;AACA,OAJA,MAIA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,QAAA,GAAA,UAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,GAAA,GAAA,KAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,KAAA,GAAA,WAAA;AACA;AACA;AAxEA,GAzEA;AAmJA,EAAA,QAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,UAAA,IAAA,GAAA;AACA,QAAA,QAAA,EAAA,KADA;AACA;AACA,QAAA,WAAA,EAAA,SAFA;AAEA;AACA,QAAA,QAAA,EAAA,KAHA;AAIA,QAAA,YAAA,EAAA,KAJA;AAKA,QAAA,WAAA,EAAA;AALA,OAAA;AAOA,aAAA,IAAA;AACA;AAVA,GAnJA;AA+JA,EAAA,SA/JA,uBA+JA;AACA,IAAA,MAAA,CAAA,mBAAA,CAAA,QAAA,EAAA,KAAA,SAAA;AACA;AAjKA,CAAA","sourcesContent":["<template>\r\n  <!--面包屑和头部按钮  -->\r\n<!--侧边栏-->\r\n    <el-container>\r\n      <div>\r\n        <el-aside :width=\"isCollapseA?'0px':'200px'\" class=\"my-article-aside\">\r\n          <el-menu :collapse=\"isCollapseA\"\r\n                   :collapse-transition=\"true\" :router=\"true\">\r\n            <el-card style=\"min-height: 468px;\" shadow=\"never\" id=\"directoryDiv\">\r\n              <div slot=\"header\" style=\"font-size: 15px\"><b>{{article.articleTitle}}</b></div>\r\n              <div v-for=\"(item, index) in aData\">\r\n                <el-button\r\n                    type=\"text\"\r\n                    size=\"small\"\r\n                    text-align=\"left\"\r\n                    @click=\"url(item.id)\">\r\n                {{item.name}}\r\n                </el-button>\r\n              </div>\r\n            </el-card>\r\n          </el-menu>\r\n        </el-aside>\r\n      </div>\r\n      <el-main>\r\n        <!--文章显示-->\r\n        <mavon-editor class=\"md\"\r\n                      :value=\"article.articleText\"\r\n                      :subfield = \"prop.subfield\"\r\n                      :defaultOpen = \"prop.defaultOpen\"\r\n                      :toolbarsFlag = \"prop.toolbarsFlag\"\r\n                      :editable=\"prop.editable\"\r\n                      :scrollStyle=\"prop.scrollStyle\"\r\n        >\r\n        </mavon-editor>\r\n        <!--评论区-->\r\n        <el-container direction=\"vertical\">\r\n          <div v-for=\"(comment,index) in commentList\" :key=\"comment\">\r\n            <el-card class=\"box-card\">\r\n              <div>\r\n                <el-avatar class=\"header-img\" :size=\"40\">\r\n                  <img v-if=\"commentImg[index]\" :src=\"require('../static/img/'+commentImg[index])\"   />\r\n                </el-avatar>\r\n\r\n                <div class=\"author-info\">\r\n                  <span class=\"author-name\">{{comment.reviewerName}}</span>\r\n                  <span class=\"author-time\">{{comment.commentTime}}</span>\r\n                </div>\r\n              </div>\r\n              <el-divider></el-divider>\r\n              <div class=\"my-comment\">{{comment.commentText}}</div>\r\n              <div class=\"my-button\" align=\"right\"\r\n                   v-if=\"comment.reviewerId==user.id||user.id==visitorId\">\r\n                <el-button type=\"text\" @click=\"deleteComment\">\r\n                  <i class=\"el-icon-delete\">\r\n                  </i>\r\n                </el-button>\r\n              </div>\r\n            </el-card>\r\n          </div>\r\n          <!--发布评论-->\r\n          <el-form class=\"comment-container\" :model=\"commentForm\" ref=\"commentForm\"\r\n                   :rules=\"commentRules\" label-position=\"left\">\r\n            <h4 class=\"comment-title\" align=\"left\">发布评论</h4>\r\n            <el-form-item prop=\"commentText\">\r\n              <el-input type=\"textarea\" :rows=\"3\" v-model=\"commentForm.commentText\" auto-complete=\"off\" placeholder=\"评论内容\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-button class=\"comment-button\" type=\"primary\" @click=\"submitComment\">发布</el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n        </el-container>\r\n      </el-main>\r\n    </el-container>\r\n</template>\r\n\r\n<script>\r\nimport 'github-markdown-css/github-markdown.css'\r\nimport 'element-ui/lib/theme-chalk/base.css';\r\nimport CollapseTransition from 'element-ui/lib/transitions/collapse-transition';\r\nimport Vue from 'vue'\r\nVue.component(CollapseTransition.name, CollapseTransition)\r\nexport default {\r\n  name : 'showArticle',\r\n  watch: {\r\n    commentList: {\r\n      handler : (val,olVal) =>{\r\n      },\r\n      deep: true\r\n    },\r\n    isCollapseA: {\r\n      handler : (val,olVal) =>{\r\n      },\r\n      deep: true\r\n    },\r\n    $route(to, from) {\r\n      this.isCollapseA = this.$route.query.isCollapseA;\r\n    },\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      isCollapseA:true,\r\n      visitorId:window.sessionStorage.getItem(\"visitingId\"),\r\n      user: {\r\n        id:window.sessionStorage.getItem(\"userId\"),\r\n        username:window.sessionStorage.getItem(\"username\"),\r\n      },\r\n      // article:this.$route.query.article,\r\n      article:{\r\n          articleId:1,\r\n          articleText:'',\r\n          articleTitle:'',\r\n          articleInfo:'',\r\n          userId:'',\r\n      },\r\n      aData:[],\r\n      commentImg:[],\r\n      commentList:[],\r\n      // commentImg:this.$route.query.commentImg,\r\n      // commentList :this.$route.query.commentList,\r\n\r\n      //评论格式\r\n      commentForm: {\r\n        commentId:'',\r\n        reviewerId:'' ,\r\n        articleId:'',\r\n        commentText:'',\r\n        commentTime:''\r\n      },\r\n      //信息格式\r\n      infoForm: {\r\n        postUserId:window.sessionStorage.getItem(\"userId\"),\r\n        receiveUserId:'',\r\n        type:'',\r\n        articleId:'',\r\n        isRead:0\r\n      },\r\n      //评论规则\r\n      commentRules: {\r\n        commentText: [\r\n          { required: true, message: \"评论内容不得为空\", trigger: \"blur\" },\r\n          { max: 100, message: \"长度在 100 个字符以内\", trigger: \"blur\" }\r\n        ],\r\n      },\r\n    }\r\n  },\r\n  mounted() {\r\n    this.listA();\r\n    window.addEventListener(\"scroll\", this.directory);\r\n  },\r\n  created() {\r\n    this.getArticleInfo();\r\n    this.getCommendInfo();\r\n  },\r\n  methods: {\r\n    async getArticleInfo() {\r\n      this.article.articleId = window.sessionStorage.getItem(\"articleId\");\r\n      console.log(this.article.articleId);\r\n      const {data: res2} = await this.$http.post(\"article/findById\", this.article);\r\n      console.log(res2.data);\r\n      this.article = res2.data;\r\n    },\r\n    async getCommendInfo() {\r\n      const {data: res} = await this.$http.post(\"article/allComment\", this.article);\r\n      this.commentImg = res.commentImg;\r\n      this.commentList = res.comments;\r\n      this.commentForm.commentId = res.commentId;\r\n      console.log(this.commentForm);\r\n    },\r\n    url(url) {\r\n      document.querySelector('#' + url).scrollIntoView(true);\r\n    },\r\n    async deleteComment() {\r\n      const {data: res4} = await this.$http.post(\"article/deleteComment\", this.commentForm);\r\n      const {data: res2} = await this.$http.post(\"article/allComment\", this.article);\r\n      this.commentList = res2.comments;\r\n      },\r\n    listA() { //找到所有标签为a的内容,但是数字不可以\r\n      this.$nextTick(function () {\r\n        let k = document.getElementsByTagName(\"a\").length;\r\n        let t = k-8;\r\n        let m = 0;\r\n        for(let i=0; i<document.getElementsByTagName(\"a\").length; i++) {\r\n          //console.log(i,document.getElementsByTagName(\"a\")[i].id,\" \",document.getElementsByTagName(\"a\")[i].parentNode.innerText);\r\n          if (document.getElementsByTagName(\"a\")[i].id !== '') {\r\n            m = m+1;\r\n            let aDat = {\r\n              'id' : document.getElementsByTagName(\"a\")[i].id,  // 获取a标签的id\r\n              'name' : document.getElementsByTagName(\"a\")[i].parentNode.innerText//获取这个a标签父节点的文字（即我们的标题）\r\n            };\r\n            this.aData.splice(i, 1, aDat);\r\n            if(m>=(t)/2)\r\n              break;\r\n          }\r\n        }\r\n      })\r\n    },\r\n    async submitComment() {\r\n      //处理评论\r\n      this.commentForm.articleId = this.article.articleId;\r\n      this.commentForm.reviewerId = this.article.userId;\r\n      const {data: res1} = await this.$http.post(\"article/addComment\", this.commentForm);\r\n      //更新评论\r\n      const {data: res2} = await this.$http.post(\"article/allComment\", this.article);\r\n      this.commentList = res2.comments;\r\n      this.commentImg = res2.commentImg;\r\n      this.commentForm.commentId = res2.commentId;\r\n      //处理通知\r\n      this.infoForm.receiveUserId = this.article.userId;\r\n      this.infoForm.type = '2';\r\n      this.infoForm.articleId = this.article.articleId;\r\n      const {data: res3} = await this.$http.post(\"info/addInfo\", this.infoForm);\r\n      this.$message_success(res3);\r\n    },\r\n    directory() {\r\n      var top=document.documentElement.scrollTop||document.body.scrollTop;\r\n      var directoryDiv=document.getElementById(\"directoryDiv\");\r\n      if(top>= 164){\r\n        directoryDiv.style.position = \"fixed\";\r\n        directoryDiv.style.top = \"50px\";\r\n        directoryDiv.style.width = \"229.313px\";\r\n      }else{\r\n        directoryDiv.style.position = \"absolute\";\r\n        directoryDiv.style.top = \"0px\";\r\n        directoryDiv.style.width = \"229.313px\";\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    prop () {\r\n      let data = {\r\n        subfield: false,// 单双栏模式\r\n        defaultOpen: 'preview',//edit： 默认展示编辑区域 ， preview： 默认展示预览区域\r\n        editable: false,\r\n        toolbarsFlag: false,\r\n        scrollStyle: true\r\n      }\r\n      return data\r\n    }\r\n  },\r\n  destroyed() {\r\n    window.removeEventListener(\"scroll\", this.directory);\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.my-header{\r\n  background-color:#eaedf1;\r\n}\r\n.my-article-aside{\r\n  margin-top: 20px;\r\n  line-height: 50px;\r\n  margin-left: 10px;\r\n  min-height: 242px;\r\n}\r\n.el-col {\r\n  border-radius: 4px;\r\n}\r\n.bg-purple {\r\n  background: #d3dce6;\r\n  font-size:10px;\r\n}\r\n.bg-purple-light {\r\n  background: #e5e9f2;\r\n}\r\n.grid-content {\r\n  border-radius: 10px;\r\n  min-height: 18px;\r\n  line-height: 30px;\r\n  margin-bottom: -5px;\r\n  font-size: 15px;\r\n}\r\n.row-bg {\r\n  padding: 10px 0;\r\n  background-color: #f9fafc;\r\n}\r\n.time{\r\n  font-size:9px;\r\n  text-align: left;\r\n}\r\n.clearfix {\r\n  font-size:15px;\r\n  text-align: left;\r\n  margin-top:8px;\r\n}\r\n.box-card {\r\n  text-align: left;\r\n  padding:0px;\r\n  margin: 20px;\r\n}\r\n.author-info {\r\n  font-size:20px;\r\n  display: inline;\r\n}\r\n.header-img {\r\n  display: inline;\r\n}\r\n.author-time {\r\n  font-size:12px;\r\n  display: flex;\r\n  margin-bottom:-20px;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}